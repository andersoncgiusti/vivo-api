{
    "env": {
        "jest": true,
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-explicit-any": "off",
        "@typescript-eslint/no-explicit-any": "off",
        "semi": 1,
        "quotes": ["warn", "single"],
        "max-len": "off",
        "no-console": "warn",
        "no-undef": "off",
        "no-var": "error",
        "no-let": "off",
        "eol-last": "off",
        "no-multiple-empty-lines": "off",
        "no-unused-vars": "off",
        "no-unused-let": "off",
        "no-sequences": "off",
        "no-unused-expressions": "off",
        "consistent-return": "off",
        "no-extraneous-dependencies": "off"
    }
}
